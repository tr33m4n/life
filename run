#!/usr/local/bin/php
<?php

declare(strict_types=1);

use tr33m4n\Life\Grid\Bounds;
use tr33m4n\Life\Grid\CellFactory;
use tr33m4n\Life\Grid\Grid;

require 'vendor/autoload.php';

$bounds = new Bounds(1, 24, 1, 24);
$grid = new Grid($bounds, new CellFactory($bounds));
$grid->build();

var_dump($grid->toArray());
